models:
    User: { personId: 'id:Person', userName: string, password: string, isActive: boolean }
    Person: { nationalityNo: string, firstName: string, lastName: string, dateOfBirth: date, sex: 'enum:m,f', address: string, phone: string, email: string, imagePath: string }
    Driver: { personId: 'id:Person', created_by_user_id: 'id:User', createdDate: timestamp }
    Application: { applicant_person_id: 'id:Person', applicationDate: dateTime, application_type_id: 'id:ApplicationType', applicationStatus: 'enum:P,C,F', lastStatusDate: dateTime, paidFees: 'decimal:10,2', created_by_user_id: 'id:User' }
    ApplicationType: { applicantTypeTitle: string, applicationFees: 'decimal:10,2' }
    LocalDrivingLicenseApplication: { application_id: 'id:Application', license_class_id: 'id:LicenseClass' }
    LicenseClass: { className: string, classDescription: text, minimumAllowedAge: unsignedSmallInteger, defaultValidityLength: unsignedSmallInteger, classFees: 'decimal:10,2' }
    TestAppointment: { test_type_id: 'id:TestTypes', Local_dl_application_id: 'id:LocalDrivingLicenseApplication', appointmentDate: DateTime, paidFees: 'decimal:10,2', created_by_user_id: 'id:User', isLocked: boolean }
    TestType: { testTypeTitle: string, testTypeDescription: text, textTypeFees: 'decimal:10,2' }
    Test: { test_appointment_id: 'id:TestAppointment', testResult: boolean, notes: text, created_by_user_id: 'id:User' }
    License: { application_id: 'id:Application', driver_id: 'id:Driver', license_class_id: 'id:LicenseClass', issueDate: timestamp, expirationDate: dateTime, notes: text, paidFees: 'decimal:10,2', isActive: boolean, created_by_user_id: 'id:User' }
    DetainedLicense: { license_id: 'id:License', detainDate: timestamp, fineFees: 'decimal:10,2', created_by_user_id: 'id:User', license_class_id: 'id:LicenseClass', isReleased: boolean, releaseDate: dateTime, released_by_user_id: 'id:User nullable', release_application_id: 'id:Application' }
